generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())
  username String @unique
  passwordHash String @map("password_hash")
  name String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}